{
  "server": {
    "host": "${VSF_API_SERVER_IP}",
    "port": "${VSF_API_SERVER_PORT}"
  },
  "elasticsearch": {
    "host": "${ES_HOST}",
    "port": "${ES_PORT}",
    "indices": [
      "${ES_INDEX_NAME}"
    ],
    "apiVersion": "${ES_API_VERSION}"
  },
  "redis": {
    "host": "${REDIS_HOST}",
    "port": "${REDIS_PORT}",
    "db": "${REDIS_DB}"
  },
  "authHashSecret": "__SUPPLZ_SECRET__",
  "objHashSecret": "__SUPPLZ_SECRET__",
  "tax": {
    "defaultCountry": "RU"
  },
  "magento2": {
    "imgUrl": "${MAGE_URL_IMG}",
    "api": {
      "url": "${MAGE_URL_REST}",
      "consumerKey": "${MAGE_API_CONSUMER_KEY}",
      "consumerSecret": "${MAGE_API_CONSUMER_SECRET}",
      "accessToken": "${MAGE_API_ACCESS_TOKEN}",
      "accessTokenSecret": "${MAGE_API_ACCESS_TOKEN_SECRET}"
    }
  },
  "magento1": {},
  "imageable": {
    "whitelist": {
      "allowedHosts": [
        "${MAGE_HOST}"
      ]
    }
  }
}
